			case 11:
				imagen = new ImageIcon("src/imgCartaEsp/imagenBASTOS11");
				break;
			case 12:
				imagen = new ImageIcon("src/imgCartaEsp/imagenBASTOS12");
				break;
			}
			break;
		case ESPADAS:
			switch(valor){
			case 1:
				imagen = new ImageIcon("src/imgCartaEsp/imagenESPADAS01");
				break;
			case 2:
				imagen = new ImageIcon("src/imgCartaEsp/imagenESPADAS02");
				break;
			case 3:
