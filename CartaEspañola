				break;
			}
			break;
		case BASTOS:
			switch(valor){
			case 1:
				imagen = new ImageIcon("src/imgCartaEsp/imagenBASTOS01");
				break;
			case 2:
				imagen = new ImageIcon("src/imgCartaEsp/imagenBASTOS02");
				break;
			case 3:
				imagen = new ImageIcon("src/imgCartaEsp/imagenBASTOS03");
				break;
			case 4:
				imagen = new ImageIcon("src/imgCartaEsp/imagenBASTOS04");
				break;
