				break;
			case 7:
				imagen = new ImageIcon("src/imgCartaEsp/imagenESPADAS07");
				break;
			case 8:
				imagen = new ImageIcon("src/imgCartaEsp/imagenESPADAS08");
				break;
			case 9:
				imagen = new ImageIcon("src/imgCartaEsp/imagenESPADAS09");
