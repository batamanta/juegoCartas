import javax.swing.*;
public class CartaEspañola extends Carta{
	public enum Palo{
		ORO,
		BASTOS,
		ESPADAS,
		COPAS
	};
	private Palo palo;
	
	public CartaEspañola(int valor,Palo palo){
		super(valor);
		ImageIcon imagen = null;
		this.palo=palo;
		switch(palo){
		case ORO:
			switch(valor){
			case 1:
				imagen = new ImageIcon("src/imgCartaEsp/imagenORO01.jpg");
				break;
			case 2:
				imagen = new ImageIcon("src/imgCartaEsp/imagenORO02.jpg");
				break;
			case 3:
				imagen = new ImageIcon("src/imgCartaEsp/imagenORO03");
